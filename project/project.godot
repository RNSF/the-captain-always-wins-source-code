config_version=5

[application]

config/name="The Captain Always Wins"
run/main_scene="res://scenes/start.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://sprites/Icon.png"
config/windows_native_icon="res://sprites/Icon.ico"

[autoload]

Init="*res://singletons/init.gd"
Debug="*res://singletons/debug.gd"
Settings="*res://singletons/settings.gd"
GameMaster="*res://singletons/game_master.gd"
SceneManager="*res://singletons/scene_manager.gd"
Dialogue="*res://singletons/dialogue.gd"
Palette="*res://singletons/palette.gd"
LiarsDice="*res://singletons/liars_dice.gd"
CollisionLayer="*res://singletons/collision_layer.gd"
Progress="*res://singletons/progress.gd"

[debug]

file_logging/enable_file_logging=true
gdscript/warnings/unused_parameter=0
gdscript/warnings/untyped_declaration=1

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray()

